# Kconfig settings for SAM3x8e processors

if MACH_STM32F1XX

config STM32F1XX_SELECT
    bool
    default y
    select HAVE_GPIO
    select HAVE_GPIO_ADC

config BOARD_DIRECTORY
    string
    default "stm32f1xx"

config CLOCK_FREQ
    int
    default 42000000 # 84000000/2

config SERIAL
    bool
    default y
config SERIAL_BAUD
    depends on SERIAL
    int "Baud rate for serial port"
    default 250000

endif
